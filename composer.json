{
	"name": "choctaw-nation/judicial-interactivity-blocks",
	"description": "Adds custom Gutenberg blocks for the Choctaw Judicial site that use the Interactivity API.",
	"type": "project",
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"phpcs": "phpcs inc/ cno-plugin-judicial-interactivity-blocks.php",
		"phpcbf": "phpcbf inc/ cno-plugin-judicial-interactivity-blocks.php"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "*"
	}
}
